{% extends 'base.html.twig' %}

{% block title %}Administration des jobs{% endblock %}

{% block body %}
<div>
    <h1>Jobs</h1>

    <ul>
        {% for job in jobs %}
            <li><a href="{{ path('admin_job_edit', {'job': job.id}) }}">{{ job.name }}</a></li>
        {% endfor %}
    </ul>

    {{ form(formJob, {action: path('admin_job_new')}) }}
</div>
{% endblock %}
